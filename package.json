{
  "name": "pc-app",
  "version": "28.1.0",
  "main": "dist-electron/main/index.js",
  "description": "Really simple Electron + Vue + Vite boilerplate.",
  "author": "zhuxietong",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "type": "module",
  "scripts": {
    "index": "tsx index.ts",
    "dev": "vite",
    "tiktok": "tsx electron/task/tiktok/test.ts",
    "build:mac": "vue-tsc --noEmit && vite build && electron-builder",
    "build": "vue-tsc --noEmit && vite build && electron-builder --win --x64",
    "build:win": "vue-tsc --noEmit && vite build && electron-builder --win --x64 --arm64",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^22.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "chai": "^5.1.1",
    "electron": "^29.1.1",
    "electron-builder": "^25.0.5",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "mocha": "^10.7.3",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vite": "^5.1.5",
    "vite-plugin-electron": "^0.28.4",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6"
  },
  "build": {
    "appId": "com.go.pc.app",
    "productName": "pc-app",
    "directories": {
      "output": "release/${version}"
    },
    "extraResources": [
      {
        "from": "C:\\Users\\zhuxietong\\AppData\\Local\\ms-playwright\\chromium-1129\\chrome-win",
        "to": "chrome-win",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "@playwright/test": "^1.46.0",
    "playwright": "^1.46.0",
    "playwright-core": "^1.46.0",
    "uuid": "^10.0.0"
  }
}
